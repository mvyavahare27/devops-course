package com.intro;

public class Person {
	
	private String name;
	
	private Car car;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Car getCar() {
		return car;
	}

	

	@Override
	public String toString() {
		return "Person [name=" + name + ", car=" + car + "]";
	}

	
	
	

}
